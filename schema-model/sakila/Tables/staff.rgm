  ]
  referencedTable = "sakila"."address"
  referencedColumns = [
    "address_id"
  ]
  onUpdate = CASCADE
  onDelete = RESTRICT
}

foreignKey "sakila"."staff"."fk_staff_store" {
  columns = [
    "store_id"
  ]
  referencedTable = "sakila"."store"
  referencedColumns = [
    "store_id"
  ]
  onUpdate = CASCADE
  onDelete = RESTRICT
}

index "sakila"."staff"."idx_fk_address_id" {
  columns = <
    {
      name = address_id
    }

  >
  type = BTREE
}

index "sakila"."staff"."idx_fk_store_id" {
  columns = <
    {
      name = store_id
    }

  >
  type = BTREE
}

